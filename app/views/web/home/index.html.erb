<h1>HELLOOOO</h1>
<div>
  <nav>
      <% if session[:user_id] %>
        <p>Welcome, <%= User.find(session[:user_id]).name %></p>
        <%= link_to 'Logout', logout_path, data: { turbo_method: :delete } %>
      <% else %>
        <%= button_to 'Войти', auth_request_path('github'), method: :post, data: { turbo: false } %>
      <% end %>
    </nav>
</div>